{
    "name": "burst-template",
    "version": "6.5.0",
    "description": "Burst is the perfect template for react SPA's.",
    "author": "arziburst@gmail.com",
    "keywords": [
        "create-react-app",
        "template",
        "webpack",
        "typescript",
        "react",
        "redux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/Arziburst/Burst.git"
    },
    "scripts": {
        "start": "cross-env-shell NODE_ENV=development ts-node -P \"./tsconfig.webpack.json\" \"./scripts/webpack/start\"",
        "build": "cross-env-shell NODE_ENV=production ts-node -P \"./tsconfig.webpack.json\" \"./scripts/webpack/build\"",
        "gen": "cross-env-shell ts-node -P \"./tsconfig.generate.json\" \"./scripts/generate\"",
        "analyze": "webpack-bundle-analyzer \"./build/stats.json\"",
        "serve": "serve \"./build\" -s",
        "clean": "rm -rf node_modules"
    },
    "dependencies": {
        "@reduxjs/toolkit": "^1.9.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-redux": "^8.0.5",
        "react-router-dom": "^6.9.0",
        "styled-components": "^5.3.9",
        "styled-reset": "^4.4.5"
    },
    "devDependencies": {
        "@redux-saga/core": "^1.2.3",
        "@soda/friendly-errors-webpack-plugin": "^1.8.1",
        "@squoosh/lib": "^0.5.3",
        "@types/app-root-path": "^1.2.4",
        "@types/node": "^18.15.3",
        "@types/react": "^18.0.28",
        "@types/react-dev-utils": "^9.0.11",
        "@types/react-dom": "^18.0.11",
        "@types/react-loadable": "^5.5.6",
        "@types/react-redux": "^7.1.25",
        "@types/react-router-dom": "^5.3.3",
        "@types/redux-logger": "^3.0.9",
        "@types/styled-components": "^5.1.26",
        "@types/uuid": "^9.0.1",
        "@types/webpack-bundle-analyzer": "^4.6.0",
        "@types/webpack-hot-middleware": "^2.25.6",
        "@types/webpack-manifest-plugin": "^3.0.5",
        "@types/webpackbar": "^4.0.3",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "app-root-path": "3.1.0",
        "babel-eslint": "^10.1.0",
        "burst-generate-files": "^0.11.18",
        "clean-webpack-plugin": "^4.0.0",
        "cross-env": "^7.0.3",
        "css-loader": "^6.7.3",
        "css-minimizer-webpack-plugin": "^4.2.2",
        "detect-port-alt": "1.1.6",
        "dotenv": "^16.0.3",
        "eslint": "^8.36.0",
        "eslint-plugin-babel": "^5.3.1",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jest": "^27.2.1",
        "eslint-plugin-react": "^7.32.2",
        "eslint-plugin-react-hooks": "^4.6.0",
        "file-loader": "^6.2.0",
        "fontmin-webpack": "^4.0.0",
        "html-webpack-plugin": "^5.5.0",
        "image-minimizer-webpack-plugin": "^3.8.2",
        "imagemin": "^8.0.1",
        "mini-css-extract-plugin": "^2.7.5",
        "react-dev-utils": "^12.0.1",
        "redux-logger": "^3.0.6",
        "redux-saga": "^1.2.3",
        "resolve-url-loader": "^5.0.0",
        "sass": "^1.59.3",
        "sass-loader": "^13.2.1",
        "serve": "^14.2.0",
